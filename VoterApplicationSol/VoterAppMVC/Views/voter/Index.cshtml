@model IEnumerable<VoterAppMVC.Models.Voter>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.votername)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.vaddress)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.votername)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.vaddress)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.voterid }) |
            @Html.ActionLink("Details", "Details", new { id=item.voterid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.voterid })
        </td>
    </tr>
}

</table>
